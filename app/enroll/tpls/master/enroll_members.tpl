{x2;if:!$userhash}{x2;include:header}<body>{x2;include:nav}<div class="container-fluid">	<div class="row-fluid">		<div class="main">			<div class="col-xs-2 leftmenu">				{x2;include:menu}			</div>			<div id="datacontent">				{x2;endif}				<div class="box itembox" style="margin-bottom:0px;border-bottom:1px solid #CCCCCC;">					<div class="col-xs-12">						<ol class="breadcrumb">							<li><a href="index.php?{x2;$_app}-master">全局</a></li>							<li><a href="index.php?{x2;$_app}-master-enroll">报名管理</a></li>							<li class="active">报名名单</li>						</ol>					</div>				</div>				<div class="box itembox" style="padding-top:20px;margin-bottom:0px;">					<h4 class="title" style="padding:10px;">						报名名单					</h4>					<form action="index.php?enroll-master-enroll-member" method="post" style="margin-top:10px;" class="form-inline">						<table class="table">							<tr>								<td style="border-top:0px;">									姓名：								</td>								<td style="border-top:0px;">									<input class="form-control" name="search[enrolltruename]" size="15" type="text" value="{x2;$search['enrolltruename']}"/>								</td>								<td style="border-top:0px;">									身份证号：								</td>								<td style="border-top:0px;">									<input class="form-control" name="search[enrollpassport]" size="15" type="text" value="{x2;$search['enrollpassport']}"/>								</td>								<td style="border-top:0px;">									手机号：								</td>								<td style="border-top:0px;">									<input class="form-control" name="search[enrollphone]" size="15" type="text" value="{x2;$search['enrollphone']}"/>								</td>								<td style="border-top:0px;">									<button class="btn btn-primary" type="submit">提交</button>									<input type="hidden" name="enbid" value="{x2;$enbat['enbid']}">									<a href="index.php?enroll-master-enroll-outmember&enbid={x2;$enbat['enbid']}{x2;$u}" class="btn btn-danger ajax">导出</a>								</td>							</tr>						</table>						<div class="input">							<input type="hidden" value="1" name="search[argsmodel]" />						</div>					</form>					<p class="alert alert-warning">批量删除时，审核通过的报名人员不能删除。</p>					<form action="index.php?enroll-master-enroll-mlite" method="post" class="form-inline">						<table class="table table-bordered table-hover">							<thead>							<tr class="info">								<th width="36"><input type="checkbox" class="checkall" target="delids"/></th>								<th width="50">									ID								</th>								<th width="180">									姓名								</th>								<th width="80">									用户ID								</th>								<th width="180">									身份证号								</th>								<th width="150">									手机号								</th>								<th>									报名时间								</th>								<th width="80">									审核状态								</th>								<th width="120">									操作								</th>							</tr>							</thead>							<tbody>							{x2;tree:$enrolls['data'],enroll,eid}							<tr>								<td><input type="checkbox" name="delids[{x2;v:enroll['enrollid']}]" value="1"></td>								<td>{x2;v:enroll['enrollid']}</td>								<td>									{x2;v:enroll['enrolltruename']}								</td>								<td>{x2;v:enroll['enrolluserid']}</td>								<td>									{x2;v:enroll['enrollpassport']}								</td>								<td>{x2;v:enroll['enrollphone']}</td>								<td>{x2;date:v:enroll['enrolltime'],'Y-m-d'}</td>								<td>									{x2;if:v:enroll['enrollverify'] == 2}<span class="text-success">已审核</span>{x2;elseif:v:enroll['enrollverify'] == 1}<span class="text-warning">待审核</span>{x2;else}<span class="text-danger">已拒绝</span>{x2;endif}								</td>								<td>									<div class="btn-group">										<a class="btn" href="index.php?{x2;$_app}-app-enroll-detail&enrollid={x2;v:enroll['enrollid']}{x2;$u}"><em class="glyphicon glyphicon-search"></em></a>										<a class="btn confirm" href="index.php?enroll-master-enroll-delenroll&enrollid={x2;v:enroll['enrollid']}{x2;$u}" title="删除"><em class="glyphicon glyphglyphicon glyphicon-remove"></em></a>									</div>								</td>							</tr>							{x2;endif}							</tbody>						</table>						<div class="control-group">							<div class="controls">								<label class="radio-inline">									<input type="radio" name="action" value="verify" checked />通过审核								</label>								<label class="radio-inline">									<input type="radio" name="action" value="passverify" />拒绝审核								</label>								<label class="radio-inline">									<input type="radio" name="action" value="unverify" />取消审核								</label>								<label class="radio-inline">									<input type="radio" name="action" value="delete" />删除								</label>								{x2;tree:$search,arg,sid}								<input type="hidden" name="search[{x2;v:key}]" value="{x2;v:arg}"/>								{x2;endtree}								<label class="radio-inline">									<button class="btn btn-primary" type="submit">提交</button>								</label>							</div>						</div>					</form>					<ul class="pagination pull-right">						{x2;$enrolls['pages']}					</ul>				</div>			</div>		</div>		{x2;if:!$userhash}	</div></div></div>{x2;include:footer}</body></html>{x2;endif}