o">							<th width="60">ID</th>							<th>用户名</th>							<th width="180">姓名</th>							<th width="180">联系电话</th>							<th width="180">邮箱</th>							<th width="180">答卷时间</th>							<th width="140">操作</th>						</tr>						</thead>						<tbody>						{x2;tree:$histories['data'],history,hid}						<tr>							<td>								{x2;v:history['syhyid']}							</td>							{x2;if:$survey['svytype']}							<td>								{x2;v:history['username']}							</td>							<td>								{x2;v:history['usertruename']}							</td>							<td>								{x2;v:history['userphone']}							</td>							<td>								{x2;v:history['useremail']}							</td>							{x2;else}							<td colspan="4">								匿名							</td>							{x2;endif}							<td>								{x2;date:v:history['syhytime'],'Y-m-d'}							</td>							<td>								<div class="btn-group">									<a class="btn" href="index.php?{x2;$_app}-master-survey-viewhistory&syhyid={x2;v:history['syhyid']}{x2;$u}" title="查看">查看</a>									<a class="btn confirm" href="index.php?{x2;$_app}-master-survey-delhistory&syhyid={x2;v:history['syhyid']}{x2;$u}" title="删除">删除</a>								</div>							</td>						</tr>						{x2;endtree}						</tbody>					</table>					<ul class="pagination pull-right">						{x2;$histories['pages']}					</ul>				</div>			</div>			{x2;if:!$userhash}		</div>	</div></div>{x2;include:footer}</body></html>{x2;endif}