<table class="table table-hover table-bordered">	<tbody>		<tr class="text-info">			<th>用户名</th>			<th>姓名</th>			<th>联系电话</th>			<th>邮箱</th>		</tr>		{x2;tree:$histories['data'],history,hid}		{x2;if:$survey['svytype']}		<tr class="info">			<td>{x2;v:history['username']}</td>			<td>{x2;v:history['usertruename']}</td>			<td>{x2;v:history['userphone']}</td>			<td>{x2;v:history['useremail']}</td>		</tr>		{x2;else}		<tr class="info">			<td colspan="4">匿名</td>		</tr>		{x2;endif}		<tr>			<td colspan="4">				{x2;eval: v:history['syhyanswers'] = unserialize(v:history['syhyanswers'])}				{x2;if:v:history['syhyanswers'][$syqnid]}				{x2;realhtml:v:history['syhyanswers'][$syqnid]}				{x2;else}				用户未填写				{x2;endif}			</td>		</tr>		{x2;endtree}	</tbody></table><ul class="pagination pull-right">	{x2;$histories['pages']}</ul><div style="clear: both"></div>